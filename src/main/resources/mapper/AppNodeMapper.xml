<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.funicorn.logger.console.mapper.AppNodeMapper">

    <select id="queryAppNode" parameterType="java.lang.String" resultType="com.funicorn.logger.console.entity.AppNode">
        select * from app_node where app_name = #{appName} and ip = #{ip} and port = #{port}
    </select>

    <update id="updateAppNodeNotDeleted" parameterType="java.lang.String">
        update app_node set deleted = '0' where id = #{id}
    </update>
</mapper>
